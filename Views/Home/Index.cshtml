<body>
    <div class="container">
        <div class="navbar">
            <img src="@Url.Content("~/PImg/Tab_image_1.jfif")" alt="" class="logo">
            <nav>
                <ul id="menuList">
                    <li><a href="@Url.Action("Create", "Home")">Home</a></li>
                    <li><a href="@Url.Action("About", "Home")">About</a></li>
                    <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
                    <li><a href="@Url.Action("Logout", "Home")">Logout</a></li>
                </ul>
            </nav>
            <img src="menu.png" alt="" class="menu-icon" onclick="togglemenu()">
        </div>

        @foreach (var item in Model)
        {
            <div class="row-3">
                <div class="col-3">
                    <div class="image">
                        <img src="@Url.Content(item.image)" height="400" width="400" class="review">
                    </div>
                    <h3 class="address"> Address - @Html.DisplayFor(modelItem => item.address)</h3>

                    <p class="location">
                        <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d14725.044470146064!2d75.7977435!3d22.6813215!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3962fd72d27719cb%3A0xcb64a96be7ac95b4!2s698-B%20VIDUR%20NAGAR%20INDORE!5e0!3m2!1sen!2sin!4v1708763652752!5m2!1sen!2sin"
                                width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"
                                referrerpolicy="no-referrer-when-downgrade"></iframe>
                    </p>

                    <button type="button">
                        <a>
                            Get
                            Now
                        </a><img src="@Url.Content("~/PImg/arrow.png")" alt="">
                    </button>

                </div>
                <div class="more-review">
                    <div>
                        <span class="house-rating__review-author"> Name -  @Html.DisplayFor(modelItem => item.name)</span>
                    </div><br />

                    <div class="house-rating__review-rating">

                        <span> Phone no.-  @Html.DisplayFor(modelItem => item.contact)</span>
                    </div><br />
                    <p class="house-rating__review-text">
                        Email -
                        @Html.DisplayFor(modelItem => item.email)
                    </p> <br />
                    <div class=" bttn">
                        <button><a href="@Url.Action("Review","Home")">Review </a></button>
                    </div>
                    <br />

                    <div class=" bttn">
                        <button><a href="@Url.Action("ViewReview","Home")">View Reviews </a></button>
                    </div>

                   
                </div>
                @Html.Raw(TempData["Delete"])
            </div>
          

        }

</body>
